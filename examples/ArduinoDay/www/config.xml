<?xml version='1.0' encoding='utf-8'?>
<widget id="net.thinkam.bluetooth.inovaprudente" version="0.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Inova Prudente - Arduino Day</name>
    <description>
        App para controlar devices via Bluetooth Serial
    </description>
    <author href="https://github.com/felipealbuquerq">
        Felipe de Almeida
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-bluetooth-serial" spec="~0.4.7" />
    <engine name="android" spec="~7.0.0" />
</widget>
